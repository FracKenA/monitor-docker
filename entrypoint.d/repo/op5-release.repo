##### op5 Monitor
[op5-monitor-updates]
name=op5 Monitor Updates
baseurl=http://repos.op5.com/el$releasever/$basearch/monitor/7/updates
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-op5

##### op5 Epel
[op5-epel]
name=op5 EPEL
baseurl=http://repos.op5.com/epel/6/$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-EPEL-6
